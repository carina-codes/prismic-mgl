{
  "format": "custom",
  "id": "post",
  "label": "Post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "published_date": {
        "type": "Date",
        "config": {
          "label": "Published Date",
          "placeholder": ""
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item,rtl"
        }
      },
      "excerpt": {
        "type": "Text",
        "config": {
          "label": "Excerpt",
          "placeholder": ""
        }
      }
    }
  }
}
